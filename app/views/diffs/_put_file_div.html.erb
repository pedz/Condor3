<!-- <%= __FILE__[Rails.root.to_s.length+1 .. -1] %> -->
<div id="<%= h which %>-title-table">
  <div id="<%= h which %>-title">
    <%= h title %>
  </div>
</div>
<div id="<%= h which %>">
  <% last_num = nil -%>
  <% lines.each do |type, num, line_id| -%>
    <% css_id, css_class, line = get_id_class_line(line_id, type, which, last_num, num) -%>
    <% last_num = num -%>
    <pre <%= css_id %> <%= css_class %> ><%= h line %></pre>
  <% end -%>
</div>
